<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
x:Class="NeMonopolia3.TeamMenu"
NavigationPage.HasNavigationBar="False"
BackgroundImage="background.png">
<ContentPage.Content>
<StackLayout Margin="20" Spacing="10" VerticalOptions="Start">
<Image Margin="25" VerticalOptions="Center" HeightRequest="200" x:Name="Iconimage" />
<Entry x:Name="id" Placeholder="Введите id команды" BackgroundColor="White" PlaceholderColor="#F7941E" TextColor="#F7941E"></Entry>
<Button  BackgroundColor="#F7941E" CornerRadius="15" TextColor="White" FontSize="16" HeightRequest="70" Text="Создать команду" ></Button>
<Button Clicked="Button_Clicked_1" BackgroundColor="#F7941E" CornerRadius="15" TextColor="White" FontSize="16" HeightRequest="70" Text="Присоединиться к команде" />
            
</StackLayout>
</ContentPage.Content>
</ContentPage>
